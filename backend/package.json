{
  "name": "paint-by-numbers",
  "version": "1.0.0",
  "description": "paint by numbers webpage",
  "main": "Main.js",
  "scripts": {
    "start": "node dist/Main.js",
    "dev": "tsc -w -p & tsc -w & nodemon dist/Main.js",
    "lint": "eslint **/*.js",
    "build": "tsc",
    "lint-fix": "eslint --fix -c .eslintrc.js 'src/**/*.{ts,js}*'",
    "style-fix": "prettier 'src/**/*' --write",
    "fix": "npm run style-fix && npm run lint-fix",
    "jest": "jest uspec --maxWorkers=2 --detectOpenHandles"
  },
  "jest": {
    "automock": false,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|src.*?\\.(ispec|uspec))\\.(ts)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "joi": "^17.12.3",
    "knex": "^3.1.0",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.2.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.0.48",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/mysql": "^2.15.26",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shanab98/paint-by-numbers.git"
  },
  "author": "Shana Beniamin"
}
